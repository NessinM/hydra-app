{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./src/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true,
    "jsx": "preserve",
    "plugins": [
      "@volar/vue-language-plugin-pug"
    ]
  },
}